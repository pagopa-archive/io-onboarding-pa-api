{
  "name": "io-onboarding-pa-api",
  "version": "0.2.0",
  "description": "IO onboarding portal webapp for public administrations",
  "repository": "https://github.com/teamdigitale/io-onboarding-pa",
  "author": "https://teamdigitale.governo.it",
  "license": "MIT",
  "scripts": {
    "clean:dist": "shx rm -rf dist",
    "clean:generated": "shx rm -rf src/generated",
    "generate:test-certs": "./scripts/generate-test-certs.sh certs",
    "pregenerate:openapi-models": "shx mkdir -p src/generated/",
    "generate:openapi-models": "gen-api-models --api-spec ./openapi.yml --out-dir src/generated --no-strict --request-types --response-decoders",
    "validate:specs": "swagger-cli validate openapi.yml",
    "prepare-db": "node dist/database/prepare.js",
    "prebuild": "npm-run-all -s clean:* generate:openapi-models",
    "watch:build": "tsc -w",
    "build": "tsc",
    "postversion": "git push && git push --tags",
    "pretest": "npm-run-all -s clean:* generate:openapi-models",
    "test": "dotenv -e env.example -- jest",
    "prettify": "prettier --write './**/*.ts'",
    "lint": "tslint --project . --exclude src/generated/**/*.ts",
    "prestart": "yarn prepare-db",
    "watch:start": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "watch": "npm-run-all -p watch:*"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.0",
    "@types/event-stream": "^3.3.34",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "10.14.1",
    "@types/node-cron": "^2.0.2",
    "@types/node-fetch": "^2.5.0",
    "@types/nodemailer": "^6.2.1",
    "@types/passport": "^0.4.0",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/pdfkit": "^0.10.3",
    "@types/pg": "^7.4.14",
    "@types/shelljs": "^0.8.5",
    "@types/tmp": "^0.1.0",
    "@types/validator": "^10.11.1",
    "danger": "^9.2.8",
    "danger-plugin-digitalcitizenship": "*",
    "dotenv-cli": "^3.0.0",
    "italia-tslint-rules": "*",
    "jest": "^24.9.0",
    "mock-fs": "^4.10.3",
    "nock": "^11.7.2",
    "nodemailer-mock-transport": "^1.3.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.12.1",
    "release-it": "^12.4.3",
    "shx": "^0.3.2",
    "sqlite3": "^4.1.0",
    "swagger-cli": "^2.3.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.1.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "cors": "^2.8.5",
    "csv-stream": "^0.2.0",
    "dotenv": "^6.1.0",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "fp-ts": "1.12.0",
    "io-spid-commons": "https://github.com/teamdigitale/io-spid-commons#v0.0.3",
    "io-ts": "1.8.5",
    "io-ts-types": "^0.4.7",
    "italia-ts-commons": "^5.1.5",
    "italia-utils": "4.1.1",
    "logform": "^2.1.2",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "pdfkit": "^0.10.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-mock": "^0.10.2",
    "soap": "^0.30.0",
    "spid-passport": "git://github.com/teamdigitale/spid-passport.git#v1.1.7",
    "tmp": "^0.1.0",
    "umzug-sync": "^1.0.0",
    "winston": "^3.2.1",
    "x509": "^0.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "resolutions": {
    "fp-ts": "1.12.0",
    "snapdragon": "^0.12.0"
  }
}
